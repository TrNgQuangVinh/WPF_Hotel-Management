<Window x:Class="Assignment1.GUI.RoomTypeList"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Assignment1.GUI"
        mc:Ignorable="d"
        Title="RoomTypeList" Height="auto" Width="auto" ResizeMode="NoResize" SizeToContent="WidthAndHeight" WindowStartupLocation="CenterScreen">
    <Grid Height="381">
        <DataGrid x:Name="dgrRoomType" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,0,0,69" Loaded="dgrRoomType_Loaded" IsReadOnly="True"/>
        <Button x:Name="btnClose" Content="Close" HorizontalAlignment="Center" Margin="0,331,0,0" VerticalAlignment="Top" Height="34" Width="104" FontSize="16" Click="btnClose_Click"/>

    </Grid>
</Window>
